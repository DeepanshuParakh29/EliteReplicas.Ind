{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/dist/index.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/dist/public/assets/$1"
    },
    {
      "src": "/.vite/manifest.json",
      "dest": "/dist/public/.vite/manifest.json"
    },
    {
      "src": "^/(.*)$",
      "dest": "/dist/public/index.html"
    }
  ]
}
